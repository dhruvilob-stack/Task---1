SELECT a.cust_id, a.cust_Name, b.cust_id AS order_cust_id, b.order_name FROM customer a LEFT JOIN orders b ON a.cust_id = b.cust_id UNION SELECT a.cust_id, a.cust_Name, b.cust_id AS order_cust_id, b.order_name FROM customer a RIGHT JOIN orders b ON a.cust_id = b.cust_id;
